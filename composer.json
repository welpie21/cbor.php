{
    "name": "welpie21/cbor.php",
    "type": "library",
    "homepage": "https://github.com/welpie21/cbor.php",
    "description": "A CBOR encoder/decoder for PHP",
    "keywords": ["cbor", "cbor-php", "cbor-encoder", "cbor-decoder"],
    "version": "2.1.0",
    "require": {
        "php": ">=8.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Beau\\CborPHP\\": "src/"
        }
    },
    "authors": [
        {
            "name": "beau",
            "email": "beaudenheijer@gmail.com"
        }
    ],
    "scripts": {
        "test-coverage": "./vendor/bin/phpunit -c phpunit.xml",
        "benchmark": "./vendor/bin/phpbench run benchmarks --report=aggregate"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.63",
        "phpunit/php-code-coverage": "^10.1",
        "phpstan/phpstan-beberlei-assert": "^1.1.2",
        "nikic/php-parser": "^v4.16.0",
        "phpbench/phpbench": "^1.2"
    }
}
